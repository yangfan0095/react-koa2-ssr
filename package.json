    "koa-static": "^3.0.0",
    "koa-static-cache": "^5.1.1",
    "koa-views": "^5.2.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
